{
    "name": "mazeworld/jwt-auth",
    "description": "JSON Web Token Authentication for Laravel",
    "require": {
        "php": "8.*",
        "ext-json": "*",
        "illuminate/contracts": "10.*",
        "illuminate/http": "10.*",
        "illuminate/support": "10.*",
        "lcobucci/jwt": "5.*",
        "namshi/jose": "7.*",
        "nesbot/carbon": "2.*",
        "illuminate/auth": "10.*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mazeworld\\JwtAuth\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Vipan Kumar",
            "email": "vipan.webdeveloper@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.*",
        "illuminate/console": "10.*",
        "illuminate/routing": "10.*",
        "orchestra/testbench": "8.*",
        "mockery/mockery": "1.*",
        "phpstan/phpstan": "1.*",
        "phpunit/phpunit": "10.*",
        "rector/rector": "0.17.*",
        "vlucas/phpdotenv": "5.*",
        "yoast/phpunit-polyfills": "2.*"
    }
}
